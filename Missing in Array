class Solution {
  public:

    // Note that the size of the array is n-1
    int missingNumber(int n, vector<int>& arr) {
        
            int sum = (n*(n+1))/2;
            int a=0;
            
            for(int i=0;i<arr.size();i++){
                a = a+arr[i];
            }
            int exp_num = sum-a;
            return exp_num;
        
    }
};
